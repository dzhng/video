    "isomorphic-unfetch": "^3.1.0",
    "lint-staged": "^10.5.3",
    "lodash": "^4.17.20",
    "lodash.throttle": "^4.1.1",
    "next": "^10.0.3",
    "nookies": "^2.5.0",
    "nprogress": "^0.2.0",
    "react": "^17.0.1",
    "react-beautiful-dnd": "^13.0.0",
    "react-dom": "^17.0.1",
    "styled-components": "^5.2.1",
    "twilio": "^3.54.0",
    "twilio-video": "^2.9.0",
    "typescript": "^4.1.2",
    "uuid": "^8.3.2",
    "yup": "^0.31.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@firebase/rules-unit-testing": "^1.1.4",
    "@testing-library/dom": "^7.28.1",
    "@testing-library/jest-dom": "^5.11.6",
    "@testing-library/react": "^11.2.2",
    "@testing-library/react-hooks": "^3.7.0",
    "@testing-library/user-event": "^12.5.0",
    "@vercel/node": "^1.8.5",
    "babel-plugin-inline-react-svg": "^1.1.2",
    "babel-plugin-styled-components": "^1.12.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-plugin-prettier": "^3.2.0",
    "jest": "^26.6.3",
    "jest-junit": "^12.0.0",
    "jest-ts-auto-mock": "^1.0.11",
    "mocha": "^8.2.1",
    "mocha-junit-reporter": "^2.0.0",
    "node-mocks-http": "^1.9.0",
    "prettier": "2.2.1",
    "react-scripts": "^4.0.1",
    "react-test-renderer": "^17.0.1",
    "start-server-and-test": "^1.11.6",
    "ts-auto-mock": "^2.7.0",
    "ts-jest": "^26.4.4",
    "ttypescript": "^1.5.12",
    "webpack": "^4.44.2"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --no-ignore --fix"
    ],
    "*.{json,md,css,scss}": [
      "prettier --write"
    ]
  },
  "scripts": {
    "dev": "NODE_OPTIONS='--inspect' next dev",
    "build": "next build",
    "start": "next start",
    "test": "sh -c 'firebase emulators:exec \"jest $0 $1 $2 $3 $4\"' '--passWithNoTests'",
    "test:dev": "sh -c 'firebase emulators:exec \"jest --watch --runInBand --reporters=default --reporters=jest-junit --coverage $0 $1 $2 $3 $4\"' '--passWithNoTests'",
    "lint": "tsc ; eslint src/**/*.{ts,tsx} --fix",
    "server": "node server.js",
    "deploy:firebase": "firebase deploy --only firestore,storage",
    "deploy:functions": "firebase deploy --only functions",
    "logs": "firebase functions:log"
  },
  "eslintConfig": {
    "root": true,
    "plugins": [
      "prettier"
    ],
    "extends": [
      "react-app",
      "prettier",
      "prettier/prettier",
      "prettier/@typescript-eslint",
      "prettier/babel",
      "prettier/react"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "ecmaVersion": 2020,
      "sourceType": "module"
    },
    "rules": {
      "no-shadow": "off",
      "@typescript-eslint/no-shadow": "warn",
      "react/react-in-jsx-scope": "off",
      "react/jsx-props-no-spreading": "off",
      "jsx-a11y/anchor-is-valid": "off",
      "jsx-a11y/alt-text": "off",
      "jsx-a11y/click-events-have-key-events": "off",
      "jsx-a11y/no-static-element-interactions": "off",
      "jsx-a11y/interactive-supports-focus": "off",
      "react/require-default-props": "off",
      "no-param-reassign": "off",
      "import/no-anonymous-default-export": "off"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "proxy": "http://localhost:8081/"
}
