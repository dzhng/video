    "@types/twilio-video": "^2.7.2",
    "@types/yup": "^0.29.10",
    "clsx": "^1.1.1",
    "concurrently": "^5.3.0",
    "cross-env": "^7.0.3",
    "d3-timer": "^2.0.0",
    "dayjs": "^1.9.7",
    "firebase": "^8.1.2",
    "firebase-admin": "^9.4.2",
    "formik": "^2.2.5",
    "formik-material-ui": "^3.0.1",
    "fscreen": "^1.1.0",
