{
  "name": "video-app",
  "version": "0.1.0",
  "private": true,
  "license": "Apache-2.0",
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@primer/octicons-react": "^9.6.0",
    "@types/d3-timer": "^2.0.0",
    "@types/fscreen": "^1.0.1",
    "@types/jest": "^26.0.14",
    "@types/lodash.throttle": "^4.1.6",
    "@types/node": "^14.11.5",
    "@types/nprogress": "^0.2.0",
    "@types/react": "^16.9.51",
    "@types/react-dom": "^16.9.8",
    "@types/twilio-video": "^2.7.1",
    "concurrently": "^5.3.0",
    "cross-env": "^7.0.2",
    "d3-timer": "^2.0.0",
    "firebase": "^7.22.1",
    "firebase-admin": "^9.2.0",
    "fscreen": "^1.1.0",
    "husky": "^4.3.0",
    "lint-staged": "^10.4.0",
    "lodash.throttle": "^4.1.1",
    "next": "^9.5.3",
    "nookies": "^2.4.0",
    "nprogress": "^0.2.0",
    "prettier": "^2.1.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "styled-components": "^5.2.0",
    "twilio": "^3.49.4",
    "twilio-video": "^2.7.2",
    "typescript": "^4.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@firebase/rules-unit-testing": "^1.1.1",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.0.4",
    "@testing-library/react-hooks": "^3.4.2",
    "@types/enzyme": "^3.10.7",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@vercel/node": "^1.8.3",
    "babel-plugin-inline-react-svg": "^1.1.1",
    "babel-plugin-styled-components": "^1.11.1",
    "cypress": "^5.3.0",
    "cypress-multi-reporters": "^1.4.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "enzyme-to-json": "^3.6.1",
    "eslint-config-prettier": "^6.12.0",
    "isomorphic-fetch": "^3.0.0",
    "jest": "^26.5.2",
    "jest-junit": "^12.0.0",
    "jest-ts-auto-mock": "^1.0.11",
    "mocha": "^8.1.3",
    "mocha-junit-reporter": "^2.0.0",
    "node-mocks-http": "^1.9.0",
    "pixelmatch": "^5.2.1",
    "puppeteer": "^5.3.1",
    "react-scripts": "4.0.0-next.98",
    "react-test-renderer": "^16.13.1",
    "start-server-and-test": "^1.11.5",
    "ts-auto-mock": "^2.6.4",
    "ts-jest": "^26.4.1",
    "ttypescript": "^1.5.12",
    "webpack": "^4.44.2"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --no-ignore --fix"
    ],
    "*.{json,md,css,scss}": [
      "prettier --write"
    ]
  },
  "scripts": {
    "dev": "NODE_OPTIONS='--inspect' next dev",
    "build": "next build",
    "start": "next start",
    "test": "sh -c 'firebase emulators:exec \"jest $0 $1 $2 $3 $4\"'",
    "test:dev": "sh -c 'firebase emulators:exec \"jest --watch --runInBand --coverage $0 $1 $2 $3 $4\"'",
    "test:ci": "jest --ci --runInBand --reporters=default --reporters=jest-junit --coverage",
    "lint": "eslint src/**/*.{ts,tsx}",
    "tsc": "tsc",
    "server": "node server.js",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run --browser chrome",
    "cypress:ci": "cross-env CYPRESS_baseUrl=http://localhost:8081 start-server-and-test server http://localhost:8081 cypress:run"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "prettier",
      "prettier/@typescript-eslint",
      "prettier/babel",
      "prettier/react"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "ecmaVersion": 2020,
      "sourceType": "module"
    },
    "rules": {
      "no-shadow": "warn",
      "react/react-in-jsx-scope": "off",
      "react/jsx-props-no-spreading": "off",
      "jsx-a11y/anchor-is-valid": "off",
      "jsx-a11y/alt-text": "off",
      "jsx-a11y/click-events-have-key-events": "off",
      "jsx-a11y/no-static-element-interactions": "off",
      "jsx-a11y/interactive-supports-focus": "off",
      "react/require-default-props": "off",
      "no-param-reassign": "off",
      "import/no-anonymous-default-export": "off"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "proxy": "http://localhost:8081/"
}
